<view class="oak-dialog {{isShowClass}}" bindtap="_maskClick" style="{{maskStyle}}">
    <oak-amimation
        name="{{animationType}}"
        duration="{{duration}}"
        bind:onAnimationEnd="animationEnd"
        show="{{isShow}}">
        <view class="oak-dialog-content dialog-class" catchtap="true" style="{{dialogStyle}}">
            <slot></slot>
        </view>
    </oak-amimation>
</view>

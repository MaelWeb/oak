.oak-stepper {
  font-size: 0;
  display: flex;
  align-items: center;
}
.oak-stepper .minus-class,
.oak-stepper .plus-class {
  width: 25px;
  height: 25px;
  border: 1rpx solid #eee;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.oak-stepper .input-class {
  font-size: 14px;
  text-align: center;
  margin: 0 5px;
}
